readtext <- function(){
assign("text.folder.path", "./park_data/" , envir = .GlobalEnv)
text.files <- list.files(path=text.folder.path, pattern = "*.txt")
assign("text.files", text.files, envir = .GlobalEnv)
assign("no.text" , length(text.files), envir = .GlobalEnv)
text.r <- vector('list', no.text)
for(i in 1:no.text){
text.files.path <- paste(text.folder.path, text.files[i], sep="")
text.r[[i]] <- readLines(text.files.path, encoding="UTF-8")
text.r[[i]] <- gsub("[[:punct:]]","",text.r[[i]]) ## 특수문자 제거
text.r[[i]] <- gsub("\n","",text.r[[i]]) ## 줄바꿔져 있는 것(enter) 제거
text.r[[i]] <- gsub("\\d", "", text.r[[i]]) ## 모든 숫자 제거
}
assign('park.old', text.r, envir = .GlobalEnv)
}
